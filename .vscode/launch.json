{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "PowerShell Launch Script",
      "type": "PowerShell",
      "request": "launch",
      "script": "bundle exec jekyll serve",
      "cwd": "${cwd}"
    },
    {
      "name": "PowerShell Attach",
      "type": "PowerShell",
      "request": "attach",
      "runspaceId": 1,
      "runspaceName": "PowerShell Interactive Console"
    },
    {
      "name": "PowerShell Launch Script (Incremental)",
      "type": "PowerShell",
      "request": "launch",
      "script": "bundle exec jekyll serve --incremental",
      "cwd": "${cwd}"
    }
  ]
}
